    # --- Step X: Mexico ISO 9997 ---
    col_F = df_base.columns[5]    # columna 6 → índice 5
    col_CD = df_base.columns[85]  # columna 86 → índice 85

    df_mexico_iso = df_base[(df_base[col_F] == "Mexico") & (df_base[col_CD] == 9997)].copy()
    write_category(df_mexico_iso, "Mexico Iso 9997", writer, sheet, new_columns)


    df_mexico_iso = df_base[(df_base[col_F] == "Mexico") & (df_base[col_CD] == "9997")].copy()
